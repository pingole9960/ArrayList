#include<iostream>
#include<vector>
using namespace std;

bool TwoSum(vector<int>&arr,int n,int k){
	sort(arr.begin(),arr.end());
	int left=0,right=n-1;
	while(left<=right){
		int sum=arr[left]+arr[right];
		if(sum==k)
			return true;
		else if(sum<k) left++;
		else
		       right--;
	}
	return false;
}


int main(int argc, char *argv[])
{
	int n;
	cin>>n;
	int k=9;
	vector<int>arr(n);
	for(auto e:arr){
		cin>>e;
	}
	bool F= TwoSum(arr,n,k);
	cout<<F;
